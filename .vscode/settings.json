{
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "git.confirmSync": false,
    "git.autofetch": true,
    "code-runner.executorMapByFileExtension": {
        ".apl": "docker run -v /workspaces/APL-codespace:/workspaces/APL-codespace --entrypoint dyalogscript dyalog/dyalog"
    },
    "code-runner.clearPreviousOutput": true,
    "code-runner.enableAppInsights": false,
    "code-runner.ignoreSelection": false,
    "code-runner.saveFileBeforeRun": true,
    "code-runner.showRunCommandInEditorContextMenu": false,
    "code-runner.showRunCommandInExplorerContextMenu": false,
    "code-runner.showExecutionMessage": false,
    "generic-input-methods.input-methods": [
        {
            "name": "Unicode Math",
            "commandName": "text.math",
            "languages": [
                "latex"
            ],
            "triggers": [
                "\\"
            ],
            "dictionary": [
                "defaults/math.json"
            ]
        }
    ]
}